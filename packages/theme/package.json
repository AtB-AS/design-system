{
  "name": "@atb-as/theme",
  "version": "11.0.0-alpha.8",
  "private": false,
  "description": "AtB Design System Colors",
  "license": "EUPL-1.2",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "pack": "yarn pack",
    "tsc": "tsc",
    "clean": "rm -rf lib",
    "test": "echo \"Error: run tests from root\" && exit 1",
    "build": "yarn fetch-variables && tsc -p tsconfig.build.json && yarn create-css && yarn copy-css",
    "copy-css": "copyfiles -u 1 \"src/**/*.{css,woff,woff2,ttf,eot,svg,png}\" lib/",
    "create-css": "tsx ./tools/create-css.ts",
    "fetch-variables": "tsx ./tools/fetch-figma-variables.ts"
  },
  "devDependencies": {
    "@figma/rest-api-spec": "^0.16.0",
    "@types/node": "^14.14.34",
    "copyfiles": "^2.4.1",
    "style-dictionary": "^4.0.0",
    "tsx": "^4.16.2",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@tfk-samf/figma-to-dtcg": "0.3.6",
    "hex-to-rgba": "^2.0.1",
    "ts-deepmerge": "^4.0.0",
    "typescript": "^4.4.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "ab76f7020a65255c8403be41aabf769d518c7621"
}
